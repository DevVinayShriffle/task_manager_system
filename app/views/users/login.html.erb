<h1>Login</h1>
<%# login  %>
<%= form_with(url: login_user_users_path, method: :post) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, placeholder: "Enter your email" %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password, placeholder: "Enter your password" %>
  </div>

  <div>
    <%= form.submit "Log In" %>
  </div>
<% end %>
<p>New user? <%= link_to "Sign up now!", root_path %></p>